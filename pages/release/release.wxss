/* /pages/release/release.wxss */
page{
    background: #ededed;
}
.content{
   margin-top: 20rpx;
}
.contentbox{
  display: flex;
  padding: 20rpx;
  border-bottom: 1px solid #ccc;
  background: #fff;
 
}
.con-img{
  width: 200rpx;
}
.con-img image{
  width: 100%;
  height: 100%;
}
.con-title{
  flex:1;
  margin-left: 20rpx;
 
}
.c-price{
  display: flex;
  justify-content: space-between
}
.c-title{
  height: 80rpx;
  font-size: 12px;
  overflow: hidden
 
}
.c-address{
  display: flex;
  justify-content: space-between;
  font-size: 12px;
  padding: 10rpx 0;
  color: #999;
}
.userimg{
  width: 40rpx;
  height: 40rpx;
  border-radius: 25rpx;
}
.weui-search-bar{
  position: fixed;
  top:0;
  left: 0;
  width: 750rpx;
  z-index: 1;
}
.usertext{
  display: flex;
  justify-content: center;
  align-items: center;
}
.btn{
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.btn view{
  
  border-radius: 15rpx;
  padding: 10rpx 20rpx;
  font-size: 12px;
}
.upbtn{
   
}
.delbtn{
 border: 1px solid red;
   color: red;
}